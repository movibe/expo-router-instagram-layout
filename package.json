{
  "main": "expo-router/entry",
  "name": "instagram-layout",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "storybook:native": "sb-rn-get-stories --config-path .storybook/native && bunx expo start",
    "start": "sb-rn-get-stories --config-path .storybook/native && bunx expo start",
    "android": "bun run start --android",
    "ios": "bun run start --ios",
    "web": "bun run start --web",
    "build": "bunx expo export -p web",
    "deploy": "vercel build && vercel deploy --prebuilt",
    "storybook:web": "start-storybook --config-dir .storybook/web -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write $PWD/\"src/**/*.{ts,tsx}\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx ",
    "lint:ci": "eslint ./src --quiet --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "test:ci": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "g": "scaffdog generate"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/core": "6.4.9",
    "@react-navigation/native": "6.1.7",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.14",
    "expo": "^49.0.7",
    "expo-build-properties": "~0.8.3",
    "expo-constants": "~14.4.2",
    "expo-firebase-analytics": "^8.0.0",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-router": "^2.0.8",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18n-js": "^4.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-web": "~0.19.9",
    "sass": "^1.62.1",
