{
  "main": "expo-router/entry",
  "name": "instagram-layout",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "storybook:native": "sb-rn-get-stories --config-path .storybook/native && EXPO_PUBLIC_STORYBOOK_ENABLED=true npx expo start",
    "start": "sb-rn-get-stories --config-path .storybook/native && npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "build": "npx expo export -p web",
    "deploy": "vercel build && vercel deploy --prebuilt",
    "storybook:web": "start-storybook --config-dir .storybook/web -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write $PWD/\"src/**/*.{ts,tsx}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:ci": "eslint . --quiet --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "g": "scaffdog generate"
  },
  "dependencies": {
    "@types/react": "~18.2.14",
    "expo": "^49.0.7",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "nativewind": "2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "~0.19.6",
    "sass": "^1.62.1",
    "typescript": "*"
  },
  "devDependencies": {
    "scaffdog": "3.0.0",
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.4",
    "@storybook/addon-ondevice-backgrounds": "^6.5.4",
    "@storybook/addon-ondevice-controls": "^6.5.4",
    "@storybook/addon-ondevice-notes": "^6.5.4",
    "@storybook/addon-react-native-web": "^0.0.21",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.16",
    "@storybook/react-native": "^6.5.4",
    "@types/react-native": "0.72.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@typescript-eslint/parser": "5.40.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.3.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsx-expressions": "1.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.00",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-testing-library": "6.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "prettier": "3.0.1",
    "tailwindcss": "3.3.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "@react-native-async-storage/async-storage",
        "@react-native-community/datetimepicker"
      ]
    }
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2",
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2",
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  }
}